{
  "name": "egg-async-validator",
  "version": "0.0.1",
  "description": "async validate plugin base on async-validator",
  "eggPlugin": {
    "name": "validate"
  },
  "main": "./lib/validator",
  "typings": "./lib/validator.d.ts",
  "files": [
    "lib",
    "app"
  ],
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "validate",
    "async-validator"
  ],
  "dependencies": {
    "async-validator": "^1.8.1"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "egg": "^1.0.0-rc.2",
    "egg-bin": "^2.2.3",
    "egg-ci": "^1.3.0",
    "egg-mock": "^3.0.1",
    "eslint": "^3.16.1",
    "eslint-config-egg": "^3.2.0",
    "supertest": "^3.0.0",
    "tslint": "~4.0.0",
    "typescript": "^2.3.4",
    "webstorm-disable-index": "^1.1.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "tsc": "tsc -d",
    "test": "npm run tsc && npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "tslint \"lib/**/*.ts\" \"app/**/*.ts\"",
    "ci": "npm run tsc && npm run lint && npm run cov",
    "pub": "npm run tsc && npm publish",
    "autod": "autod"
  },
  "ci": {
    "version": "6, 7",
    "license": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/egg-async-validator.git"
  },
  "bugs": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "homepage": "https://github.com/eggjs/egg-async-validator#readme",
  "author": "fengmk2",
  "license": "MIT"
}
